   "source": [
    "# 1. 함수를 매개변수로 전달\n",
    "def call_func(func):\n",
    "    for i in range(1,4):\n",
    "        func(i)\n",
    "\n",
    "def print_num(n):\n",
    "    print(n,\"!!\")\n",
    "\n",
    "call_func(print_num)    # 매개변수로 함수를 넣음"
